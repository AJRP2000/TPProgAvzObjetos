CREATE CACHED TABLE TURNOS(IDTURNO INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DNIPACIENTE INTEGER,IDTERAPEUTA INTEGER,FECHA TIMESTAMP,ESTADO VARCHAR(20))
