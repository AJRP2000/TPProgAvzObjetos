CREATE CACHED TABLE TERAPEUTAS(IDTERAPEUTA INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NOMBRE VARCHAR(50),HORARIO VARCHAR(50))
